package colecao;

import java.util.HashSet;
import java.util.Set;

public class Conjunto {
	
	public static void main(String[] args) {
		
		//Conjunto heterogêneo (Aceita vários tipos de valores)
		Set hetero = new HashSet();
		
		hetero.add(2.17);
		hetero.add(null);
		hetero.add("Strings");
		
		System.out.println(hetero);
		
		
	}

}
